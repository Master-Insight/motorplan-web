{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@ico/*": [
        "src/icons/*"
      ],
      "@config": [
        "config.js"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}